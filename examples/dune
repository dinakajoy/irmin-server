(executables
 (names server)
 (libraries irmin-server)
 (modules server)
 (preprocess
  (pps ppx_irmin)))

(executables
 (names ping tree)
 (libraries irmin-client)
 (modules ping tree)
 (preprocess
  (pps ppx_irmin)))

(alias
 (name examples)
 (deps server.exe ping.exe tree.exe))

(alias
 (name runtest)
 (package irmin-server)
